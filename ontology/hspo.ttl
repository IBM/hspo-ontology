rdfs:displayName rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#cardinality
owl:cardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://research.ibm.com/ontologies/hspo/belongsToAgeGroup
:belongsToAgeGroup rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Age ;
                   rdfs:range :AgeGroup ;
                   rdfs:comment "The age group the person belongs to"@en ;
                   rdfs:label "belongsToAgeGroup"@en .


###  http://research.ibm.com/ontologies/hspo/followsReligion
:followsReligion rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Person ;
                 rdfs:range :Religion ;
                 rdfs:comment "The religon a person follows"@en ;
                 rdfs:label "followsReligion"@en .


###  http://research.ibm.com/ontologies/hspo/hasAge
:hasAge rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Person ;
        rdfs:range :Age ;
        rdfs:comment "The age of a person"@en ;
        rdfs:label "hasAge"@en .


###  http://research.ibm.com/ontologies/hspo/hasBehaviour
:hasBehaviour rdf:type owl:ObjectProperty ;
              rdfs:domain :Person ;
              rdfs:range :Behaviour ;
              rdfs:comment "A behaviour for a Person. E.g. diet, smoking, exercise, etc."@en ;
              rdfs:label "hasBehaviour"@en .


###  http://research.ibm.com/ontologies/hspo/hasDisease
:hasDisease rdf:type owl:ObjectProperty ;
            rdfs:domain :Person ;
            rdfs:range :Disease ;
            rdfs:comment "The disease associated with a Person"@en ;
            rdfs:label "hasDisease"@en .


###  http://research.ibm.com/ontologies/hspo/hasDuration
:hasDuration rdf:type owl:ObjectProperty ;
             rdfs:domain :Intervention ;
             rdfs:range :Duration ;
             rdfs:comment "The duration of an intervention"@en ;
             rdfs:label "hasDuration"@en .


###  http://research.ibm.com/ontologies/hspo/hasEvidence
:hasEvidence rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Behaviour
                                         :Disease
                                         :Intervention
                                         :SocialContext
                                       )
                         ] ;
             rdfs:range :Evidence ;
             rdfs:comment "Relates a social problem, disease or intervention to an evidence, including provenance, confidence score and original knowledge"@en ;
             rdfs:label "hasEvidence"@en .


###  http://research.ibm.com/ontologies/hspo/hasEvidenceProvenance
:hasEvidenceProvenance rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :Evidence ;
                       rdfs:range :EvidenceProvenance ;
                       rdfs:comment "Relates the evidence to its provenance"@en ;
                       rdfs:label "hasEvidenceProvenance"@en .


###  http://research.ibm.com/ontologies/hspo/hasEvidenceSource
:hasEvidenceSource rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf prov:wasDerivedFrom ;
                   rdfs:domain :EvidenceProvenance ;
                   rdfs:range :EvidenceSource ;
                   rdfs:comment "The source of the evidence"@en ;
                   rdfs:label "hasEvidenceSource"@en .


###  http://research.ibm.com/ontologies/hspo/hasGender
:hasGender rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range :Gender ;
           rdfs:comment "The gender of a person"@en ;
           rdfs:label "hasGender"@en .


###  http://research.ibm.com/ontologies/hspo/hasIntervention
:hasIntervention rdf:type owl:ObjectProperty ;
                 rdfs:domain :Person ;
                 rdfs:range :Intervention ;
                 rdfs:comment "An intervention for a Person"@en ;
                 rdfs:label "hasIntervention"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionOutcome
:hasInterventionOutcome rdf:type owl:ObjectProperty ;
                        rdfs:domain :Intervention ;
                        rdfs:range :InterventionOutcome ;
                        rdfs:comment "Relates an outcome to an intervention"@en ;
                        rdfs:label "hasInterventionOutcome"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionOutcomeMetric
:hasInterventionOutcomeMetric rdf:type owl:ObjectProperty ;
                              rdfs:domain :InterventionOutcome ;
                              rdfs:range :InterventionOutcomeMetric ;
                              rdfs:comment "The metric to measure an outcome."@en ;
                              rdfs:label "hasInterventionOutcomeMetric"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionOutcomeResult
:hasInterventionOutcomeResult rdf:type owl:ObjectProperty ;
                              rdfs:domain :InterventionOutcome ;
                              rdfs:range :InterventionOutcomeResult ;
                              rdfs:comment "The outcome result scale."@en ;
                              rdfs:label "hasInterventionOutcomeResult"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionOutcomeType
:hasInterventionOutcomeType rdf:type owl:ObjectProperty ;
                            rdfs:domain :InterventionOutcome ;
                            rdfs:range :InterventionOutcomeType ;
                            rdfs:comment "The type of Intervention outcome."@en ;
                            rdfs:label "hasInterventionOutcomeType"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionProvider
:hasInterventionProvider rdf:type owl:ObjectProperty ;
                         rdfs:domain :Intervention ;
                         rdfs:range :InterventionProvider ;
                         rdfs:comment "Intervention provider of an intervention"@en ;
                         rdfs:label "hasInterventionProvider"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionStatus
:hasInterventionStatus rdf:type owl:ObjectProperty ;
                       rdfs:domain :Intervention ;
                       rdfs:range :InterventionStatus ;
                       rdfs:label "hasInterventionStatus"@en .


###  http://research.ibm.com/ontologies/hspo/hasInterventionType
:hasInterventionType rdf:type owl:ObjectProperty ;
                     rdfs:domain :Intervention ;
                     rdfs:range :InterventionType ;
                     rdfs:label "hasInterventionType"@en .


###  http://research.ibm.com/ontologies/hspo/hasMaritalStatus
:hasMaritalStatus rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Person ;
                  rdfs:range :MaritalStatus ;
                  rdfs:comment "The marital status of a person"@en ;
                  rdfs:label "hasMaritalStatus"@en .


###  http://research.ibm.com/ontologies/hspo/hasRaceOrEthnicity
:hasRaceOrEthnicity rdf:type owl:ObjectProperty ;
                    rdfs:domain :Person ;
                    rdfs:range :RaceAndEthnicity ;
                    rdfs:comment "The race and/or ethnicity of a person"@en ;
                    rdfs:label "hasRaceOrEthnicity"@en .


###  http://research.ibm.com/ontologies/hspo/hasSocialContext
:hasSocialContext rdf:type owl:ObjectProperty ;
                  rdfs:domain :Person ;
                  rdfs:range :SocialContext ;
                  rdfs:comment "The social context of a person"@en ;
                  rdfs:label "hasSocialContext"@en .


###  http://research.ibm.com/ontologies/hspo/hasStageOfLife
:hasStageOfLife rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Age ;
                rdfs:range :StageOfLife ;
                rdfs:comment "The stage of life of a person"@en ;
                rdfs:label "hasStageOfLife"@en .


###  http://research.ibm.com/ontologies/hspo/isObject
:isObject rdf:type owl:ObjectProperty ;
          rdfs:domain :Evidence ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Age
                                     :Behaviour
                                     :Disease
                                     :Gender
                                     :Intervention
                                     :Location
                                     :MaritalStatus
                                     :RaceAndEthnicity
                                     :Religion
                                     :SocialContext
                                   )
                     ] ;
          rdfs:comment "Component of n-ary evidence where its role in the evidence is an object"@en ;
          rdfs:label "isObject"@en .


###  http://research.ibm.com/ontologies/hspo/isPartOfEvidence
:isPartOfEvidence rdf:type owl:ObjectProperty ;
                  rdfs:domain :Evidence ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :Age
                                             :Behaviour
                                             :Disease
                                             :Gender
                                             :Intervention
                                             :Location
                                             :MaritalStatus
                                             :RaceAndEthnicity
                                             :Religion
                                             :SocialContext
                                           )
                             ] ;
                  rdfs:comment "Component of n-ary evidence where its role in the evidence is undefined "@en ;
                  rdfs:label "isPartOfEvidence"@en .


###  http://research.ibm.com/ontologies/hspo/isSubject
:isSubject rdf:type owl:ObjectProperty ;
           rdfs:domain :Evidence ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Age
                                      :Behaviour
                                      :Disease
                                      :Gender
                                      :Intervention
                                      :Location
                                      :MaritalStatus
                                      :RaceAndEthnicity
                                      :Religion
                                      :SocialContext
                                    )
                      ] ;
           rdfs:comment "Component of n-ary evidence where its role in the evidence is a subject"@en ;
           rdfs:label "isSubject"@en .


###  http://research.ibm.com/ontologies/hspo/lives
:lives rdf:type owl:ObjectProperty ;
       rdfs:domain :Person ;
       rdfs:range :Location ;
       rdfs:comment "The location where the person lives, e.g. address, state, area, country, etc. "@en ;
       rdfs:label "lives"@en .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://research.ibm.com/ontologies/hspo/age_in_years
:age_in_years rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Age ;
              rdfs:range xsd:int ;
              rdfs:comment "Current age of a person in years"@en ;
              rdfs:label "Age in years"@en .


###  http://research.ibm.com/ontologies/hspo/alignment_score
:alignment_score rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Evidence ;
                 rdfs:range xsd:double ;
                 rdfs:comment "Alignment score between the Evidence and the Person Graph"@en ;
                 rdfs:label "Alignment score"@en .


###  http://research.ibm.com/ontologies/hspo/confidence_score
:confidence_score rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Evidence ;
                  rdfs:range xsd:double ;
                  rdfs:comment "Confidence score of the Evidence. It may include probablity of each source as well as datasource trust scores"@en ;
                  rdfs:label "Confidence score"@en .


###  http://research.ibm.com/ontologies/hspo/intervention_code
:intervention_code rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Intervention ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The code of the intervention"@en ;
                   rdfs:label "Intervention code"@en .


###  http://research.ibm.com/ontologies/hspo/intervention_code_system
:intervention_code_system rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Intervention ;
                          rdfs:range xsd:string ;
                          rdfs:comment "The intervention coding system. E.g. CPT, RxNorm, ICD, etc."@en ;
                          rdfs:label "Intervention code system"@en .


###  http://research.ibm.com/ontologies/hspo/intervention_name
:intervention_name rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Intervention ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The name of the intervention, which may include a short textual description "@en ;
                   rdfs:label "Intervention name"@en .


###  http://research.ibm.com/ontologies/hspo/number_in_household
:number_in_household rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :Household ;
                     rdfs:range xsd:int ;
                     rdfs:comment "Number in household"@en ;
                     rdfs:label "Number in household"@en .


###  http://research.ibm.com/ontologies/hspo/person_id
:person_id rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Person ;
           rdfs:range xsd:string ;
           rdfs:comment "The ID of the person"@en ;
           rdfs:label "Person ID"@en .


###  http://research.ibm.com/ontologies/hspo/positivity_indicator
:positivity_indicator rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :SocialContext ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Positivity of the social factor. Possible values are: undefined, positive, negative, neutral, contradicting "@en ;
                      rdfs:label "Positivity indicator"@en .


###  http://research.ibm.com/ontologies/hspo/relation_label
:relation_label rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Evidence ;
                rdfs:range xsd:string ;
                rdfs:comment "Relation name(label) of the Evidence. For example, 'increases' or 'impacts'"@en ;
                rdfs:label "Relation label"@en .

###  http://research.ibm.com/ontologies/hspo/relation_type
:relation_type rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Evidence ;
                rdfs:range xsd:string ;
                rdfs:comment "High level relation type of the Evidence. For example, 'causes', 'modifies', 'unrelated'"@en ;
                rdfs:label "Relation type"@en .

###  http://research.ibm.com/ontologies/hspo/relation_polarity
:relation_polarity rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Evidence ;
                rdfs:range xsd:string ;
                rdfs:comment "The polarity of the relation in the Evidence. Possible values, 'positive', 'negative', 'neutral', 'unknown'"@en ;
                rdfs:label "Relation polarity"@en .

###  http://research.ibm.com/ontologies/hspo/relation_quantifier
:relation_quantifier rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Evidence ;
                rdfs:range xsd:string ;
                rdfs:comment "The quantifier of the relation in the Evidence."@en ;
                rdfs:label "Relation quantifier"@en .


###  http://research.ibm.com/ontologies/hspo/source_doc_id
:source_doc_id rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :EvidenceSource ;
               rdfs:range xsd:string ;
               rdfs:comment "The ID (e.g. URI) of the document the Evidence is originated from"@en ;
               rdfs:label "Source document ID"@en .


###  http://research.ibm.com/ontologies/hspo/source_name
:source_name rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EvidenceSource ;
             rdfs:range xsd:string ;
             rdfs:comment "The ID (e.g. URI) or the name of the source/corpus the Evidence is originated from"@en ;
             rdfs:label "Source name"@en .


###  http://research.ibm.com/ontologies/hspo/source_entities
:source_entities rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EvidenceSource ;
             rdfs:range xsd:string ;
             rdfs:comment "The original text snippets of the Evidence entities"@en ;
             rdfs:label "Source entities"@en .


###  http://research.ibm.com/ontologies/hspo/source_entity_cuis
:source_entity_cuis rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EvidenceSource ;
             rdfs:range xsd:string ;
             rdfs:comment "The UMLS entities of the relation in the Evidence source"@en ;
             rdfs:label "Source UMLS entities"@en .


###  http://research.ibm.com/ontologies/hspo/source_score_metric
:source_score_metric rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :EvidenceSource ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Datasource score metric of the Evidence"@en ;
                     rdfs:label "Source score metric"@en .


###  http://research.ibm.com/ontologies/hspo/source_score_value
:source_score_value rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :EvidenceSource ;
                    rdfs:range xsd:double ;
                    rdfs:comment "Source score value of the Evidence"@en ;
                    rdfs:label "Source score value"@en .


###  http://research.ibm.com/ontologies/hspo/source_text
:source_text rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EvidenceSource ;
             rdfs:range xsd:string ;
             rdfs:comment "The original text (e.g. sentence) the Evidence is originated from"@en ;
             rdfs:label "Source text"@en .


###  http://research.ibm.com/ontologies/hspo/trust_score
:trust_score rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :EvidenceSource ;
             rdfs:range xsd:double ;
             rdfs:comment "Datasource trust score for the Evidence"@en ;
             rdfs:label "Trust score"@en .


#################################################################
#    Classes
#################################################################

###  http://research.ibm.com/ontologies/hspo/Age
:Age rdf:type owl:Class ;
     :umlsCui "C0001779" ;
     rdfs:comment "The age of a person"@en ;
     rdfs:label "Age"@en .


###  http://research.ibm.com/ontologies/hspo/AgeGroup
:AgeGroup rdf:type owl:Class ;
          :umlsCui "C0027362" ;
          rdfs:comment "The age group the person belongs to"@en ;
          rdfs:label "Age group"@en .


###  http://research.ibm.com/ontologies/hspo/Behaviour
:Behaviour rdf:type owl:Class ;
           :umlsCui "" ;
           rdfs:comment "Human behaviour, e.g. diet, smoking, exercising, sleep, etc." ;
           rdfs:label "Behaviour"@en .


###  http://research.ibm.com/ontologies/hspo/BuiltEnvironment
:BuiltEnvironment rdf:type owl:Class ;
                  rdfs:subClassOf :SocialContext ;
                  :umlsCui "" ;
                  rdfs:comment "The location surrounding an individual including neighborhood." ;
                  rdfs:label "Built Environment" .


###  http://research.ibm.com/ontologies/hspo/Disease
:Disease rdf:type owl:Class ;
         owl:equivalentClass efo:EFO_0000408 ;
         :umlsCui "C0012634" ,
                  "C0221423" ,
                  "C0338067" ,
                  "C1511989" ;
         rdfs:label "Disease"@en .


###  http://research.ibm.com/ontologies/hspo/Duration
:Duration rdf:type owl:Class ;
          :umlsCui "" ;
          rdfs:comment "Duration of some action"@en ;
          rdfs:label "Duration"@en .


###  http://research.ibm.com/ontologies/hspo/Educational
:Educational rdf:type owl:Class ;
             rdfs:subClassOf :SocialContext ;
             :umlsCui "" ;
             rdfs:comment "The educational attainment and literacy of an individual." ;
             rdfs:label "Educational"@en .


###  http://research.ibm.com/ontologies/hspo/Employment
:Employment rdf:type owl:Class ;
            rdfs:subClassOf :SocialContext ;
            :umlsCui "" ;
            rdfs:comment "The labor context of an individual including unemployment." ;
            rdfs:label "Employment"@en .


###  http://research.ibm.com/ontologies/hspo/Evidence
:Evidence rdf:type owl:Class ;
          rdfs:subClassOf prov:Entity ;
          rdfs:comment "Knowledge evidence" ;
          rdfs:label "Evidence"@en .


###  http://research.ibm.com/ontologies/hspo/EvidenceProvenance
:EvidenceProvenance rdf:type owl:Class ;
                    rdfs:comment "The evidence provenance. Contains all evidence sources" ;
                    rdfs:label "Evidence provenance"@en .


###  http://research.ibm.com/ontologies/hspo/EvidenceSource
:EvidenceSource rdf:type owl:Class ;
                rdfs:subClassOf prov:Entity ;
                rdfs:comment "The source of the evidence" ;
                rdfs:label "Evidence Source"@en .


###  http://research.ibm.com/ontologies/hspo/Financial
:Financial rdf:type owl:Class ;
           rdfs:subClassOf :SocialContext ;
           :umlsCui "" ;
           rdfs:comment "The financial context of an individual including economic stability, material hardship and medical cost burden." ;
           rdfs:label "Financial" .


###  http://research.ibm.com/ontologies/hspo/Food
:Food rdf:type owl:Class ;
      rdfs:subClassOf :SocialContext ;
      :umlsCui "C0015619" ,
               "C0022901" ,
               "C0038187" ,
               "C0476605" ,
               "C0476609" ,
               "C0480157" ,
               "C0480158" ,
               "C0480159" ,
               "C0480160" ,
               "C0480161" ,
               "C0480162" ,
               "C0480163" ,
               "C0480164" ,
               "C0480165" ,
