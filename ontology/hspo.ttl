@prefix : <http://research.ibm.com/ontologies/hspo/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix efo: <http://www.ebi.ac.uk/efo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@base <http://research.ibm.com/ontologies/hspo/> .

<http://research.ibm.com/ontologies/hspo/> rdf:type owl:Ontology ;
                                            owl:imports efo:efo.owl ;
                                            dc:abstract """The Health and Social Person-centric Ontology (HSPO) describes a schema for specifying evidences about a person in a consistent way.
          The model allows but not limited to specify evidence, interventions and outcomes in health and social care domains."""@en ;
                                            dc:creator "IBM Research Europe" ;
                                            dc:publisher "IBM" ;
                                            <http://purl.org/dc/terms/created> "2022-06-02"^^xsd:date ;
                                            <http://purl.org/dc/terms/modified> "2024-02-09"^^xsd:date ;
                                            <http://purl.org/dc/terms/title> "Health and Social Person-centric Ontology (HSPO)"@en ;
                                            owl:versionInfo "0.2.0" .

